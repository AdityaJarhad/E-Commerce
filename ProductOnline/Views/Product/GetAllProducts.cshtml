<div>List of Products</div>
<hr />
@{
    List<Product> list = (List<Product>)ViewData["AllProducts"];
}
@{
    foreach(Product product in list)
    {
        <li>id : @product.Id, title : @product.Title, Description : @product.Description,Title : @product.Title,price : @product.UnitPrice,imageUrl : @product.ImageUrl,quantity : @product.Quantity</li>
    }
}